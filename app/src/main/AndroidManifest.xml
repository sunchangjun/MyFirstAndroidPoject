<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sun.ccc.myfirstandroidpoject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="傻瓜赚钱"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ThirdActivity">
            <intent-filter>
                <action android:name="com.sun.ccc.myfirstandroidpoject.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.sun.ccc.myfirstandroidpoject.MY_CAT" />
            </intent-filter>
        </activity>
        <activity android:name=".WEBActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="tel" />
            </intent-filter>
        </activity>
        <activity android:name=".OneActivity" />
        <activity android:name=".TwoActivity" />
        <activity android:name=".UIActivity" />
        <activity android:name=".ListViewActivity"></activity>
    </application>

</manifest>